    "p = 60\n",
    "\n",
    "if p > 75:\n",
    "    grade = \"O\"\n",
    "elif 60 < p <= 75:\n",
    "    grade = \"A\"\n",
    "elif 50 < p <= 60:\n",
    "    grade = \"B\"\n",
    "elif 35 < p <= 50:\n",
    "    grade = \"C\"\n",
    "else:\n",
    "    grade = \"Fail\"\n",
    "\n",
    "print(\"Grade:\", grade)"
